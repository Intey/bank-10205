.col-md-12
    h3 Транзакции
    table.table
        tbody
            tr
                th Событие
                th Дата
                th Коэфициент
                th Цена
                th Тип
            for t in transactions
                tr
                    td
                        if event.id != t.event.id
                            a(href="/events/{{t.event.id}}")=t.event
                        else
                            =t.event
                    td=t.date
                    td=t.parts
                    td=t.summ
                    td=t.type_view
